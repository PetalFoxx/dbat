{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "DBAT: (GDB) Server",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/circle",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb",
        },
        {
            "name": "DBAT: (GDB) Migrate",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/migrate",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb",
        },
    ]
}